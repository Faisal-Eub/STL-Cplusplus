#include<bits/stdc++.h>
#define faster ios::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);
#define ll long long int
using namespace std;

int main()
{
    faster

    string str1; getline(cin , str1);             // string str1; cin >> str1;
    int len = str1.size();                        // Finding string length  
    int counter = 0;
    string str2;
    str2.resize(len);                             // if I don't do this it will be segmentation error because str2 length is not fix. So next loop will crush.So only way initialize its size first 

    for (int k=0; k<len; k++)
        {
            str2[k] = 'S';
            k++;
            str2[k] = 'O';
            k++;
            str2[k] = 'S';
        }


    for (int i=0,j=0; i<len; i++,j++)
    {
            if (str1[i] != str2[j])
            {
                counter++;
            }
    }
    cout << counter ;

}
